                <h4 style="color: white;">Search Result</h4>
                <div class="bann">
                    <div class="bord" style="background-color: rgb(0, 255, 0);"></div>
                </div>
            </div>
            <div class="posting">
                <% for(let i=0; i < arr.length; i++){ %>
                    <a href="https://daddyhub.in/movie/<%= arr[i].name %> ">
                        <div class="con">
                            <img src="<%=arr[i].poster%> " alt="<%= arr[i].name %> " class="image">
                            <div class="overlay">
                                <div class="pri">
                                    <div class="print shadow-lg">
                                        <% let pcheck;%>
                                            <span>
                                                <% if(arr[i].cam_print=="y" ){ %>
                                                    <% pcheck='CAM' %>
                                                        <%= pcheck %>
                                                            <% }else{ %>
                                                                <% pcheck='HD' %>
                                                                    <%= pcheck %>
                                                                        <% } %>
                                            </span>
                                    </div>
                                </div>
                                <div class="desc">
                                    <span>
                                        <%= arr[i].name %>
                                    </span>
                                    <p>
                                        <%= arr[i].length %>
                                            <%= arr[i].genre %>
                                    </p>
                                    <p>
                                        <%= arr[i].language %> | <%= arr[i].year %>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </a>
                    <% } %>
            </div>
        </div>
        <div class="ho">
